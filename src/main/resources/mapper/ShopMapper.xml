<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.shop.ShopDAO">

	<insert id="registShop" parameterType="com.shop.Shop">
		INSERT INTO shop
		VALUES (#{isDel}, #{modDate}, #{name}, #{regDate}, #{totalMoney}, #{totalSale})
	</insert>
	
</mapper>